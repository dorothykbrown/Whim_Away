<%= simple_form_for @property do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :photo, label: 'Photo URL' %>
  <%= f.input :category, collection: [ "Treehouse", "Sailboat", "Castle"] %>
  <%= f.input :description %>
  <%= f.input :price, label: 'Price per night' %>
  <%= f.submit :Submit, class: "btn btn-primary" %>
<% end %>
