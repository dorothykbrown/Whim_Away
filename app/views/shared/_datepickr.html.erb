<%= simple_form_for :search, method: "get" do |f|%>
  <%= f.input :check_in_date, as: :string, required: false, input_html: {class: "datepicker"} %>
  <%= f.input :check_out_date, as: :string, required: false, input_html: {class: "datepicker"} %>
  <%= f.input :num_guests %>
  <%= f.button :submit, 'Go', class: "btn btn-primary" %>
  <%#= link_to 'Go', properties_available_path, class: "btn btn-primary" %>
<% end %>
